// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Payments{
  id             int       @id @default(autoincrement())
  companyId      int       @map("company_id")
  company        company   @relation(fields:[companyId],references:[id])
  value          int
  toDate         DateTime
  dueDate        DataTime
  paymentForm    String
  advertising    String
  key            int
  type           String 
}
model Company{
  id       Int @id @default(autoincrement())
  name     String
  category String
  cnpj     String
  evaluate Float
  places   String
  users User @relation(fields: [users], references: [])
  creatAt DateTime @default(now())
  creatAt DateTime @updateAt
model Adrress {
  id            Int       @id @default(autoincrement())
  place         String
  number        Int
  zipcode       String
  lat           Float
  long          Float
  url           String
}